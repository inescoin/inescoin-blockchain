use Inescoin\ES\ESBlockchainProvider;

require __DIR__.'/../../vendor/autoload.php';


$params = getopt('', [
	'prefix::',
]);


$prefix = $params['prefix'] ?? 'bob';

$esService = ESBlockchainProvider::getInstance($prefix);
$esService->resetAll();
